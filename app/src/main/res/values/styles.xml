<resources>

	<!-- Base application theme. -->
	<style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
		<!-- Customize your theme here. -->
		<item name="colorPrimary">@color/colorPrimary</item>
		<item name="colorPrimaryDark">@color/colorPrimaryDark</item>
		<item name="colorAccent">@color/colorAccent</item>
	</style>

	<style name="AlertDialogStyle" parent="Theme.AppCompat.Dialog">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
	</style>

	<style name="LoginInputStyle" parent="Widget.AppCompat.EditText">
		<item name="android:textColorHint">@color/hint_transparent</item>
		<item name="android:textColor">@color/text_color_black</item>
		<item name="android:background">@null</item>
		<item name="android:fontFamily">@font/os_semi_bold</item>
		<item name="android:textSize">@dimen/login_input_size</item>
	</style>

	<style name="DialogTheme" parent="Theme.AppCompat.Light.Dialog">
		<item name="android:windowNoTitle">true</item>
		<item name="android:windowFullscreen">false</item>
		<item name="android:windowIsFloating">false</item>
	</style>

	<style name="HelpStepTextStyle" parent="Widget.AppCompat.TextView">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:textColor">@color/black</item>
		<item name="android:fontFamily">@font/os</item>
		<item name="android:textSize">@dimen/custom_14sp</item>
	</style>

	<style name="HelpStepExplanationStyle" parent="Widget.AppCompat.TextView">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:textColor">@color/black</item>
		<item name="android:fontFamily">@font/os_bold</item>
		<item name="android:textSize">@dimen/custom_14sp</item>
	</style>

	<style name="HelpNoteStyle" parent="Widget.AppCompat.TextView">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:textAlignment">center</item>
		<item name="android:textColor">@color/black</item>
		<item name="android:fontFamily">@font/os_bold</item>
		<item name="android:textSize">@dimen/custom_14sp</item>
	</style>

	<style name="AppTheme.NumberPicker" parent="AppTheme">
		<item name="colorControlNormal"> @color/colorAccent </item>
		<item name="selectableItemBackground"> @color/colorAccent  </item>
	</style>

	<style name="TextHeadersProfile">
		<item name="android:textColor">@color/textDarkLighter</item>
		<item name="android:textSize">@dimen/titles_text</item>
	</style>

	<style name="TextValuesProfile">
		<item name="android:textColor">@color/textDark</item>
		<item name="android:textSize">@dimen/profile_values</item>
		<item name="android:layout_marginTop">@dimen/unit</item>
	</style>

	<style name="CardViewProfile">
		<item name="android:layout_margin">@dimen/unit2x</item>
		<item name="cardCornerRadius">@dimen/unit</item>
		<item name="cardElevation">@dimen/unit</item>
	</style>

	<style name="ExerciseTitle" parent="TextAppearance.AppCompat.Small">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:textSize">17sp</item>
		<item name="android:textAlignment">textStart</item>
		<item name="android:textColor">@color/black</item>
		<item name="android:fontFamily">@font/os</item>
	</style>

	<style name="CircuitExerciseTitleStyle" parent="TextAppearance.AppCompat.Small">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:fontFamily">@font/os_semi_bold</item>
		<item name="android:textSize">15sp</item>
		<item name="android:textAlignment">textStart</item>
		<item name="android:textColor">@color/black</item>
	</style>

	<style name="TopBar">
		<item name="android:layout_width">match_parent</item>
		<item name="android:layout_height">51dp</item>
		<item name="android:background">@drawable/gradient_blue</item>
		<item name="android:elevation">4dp</item>
	</style>

	<style name="TopBarTitle">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:textColor">@color/white</item>
		<item name="android:textSize">20sp</item>
		<item name="android:fontFamily">@font/os_semi_bold</item>
	</style>

	<style name="TopBarButton">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">match_parent</item>
		<item name="android:gravity">center_vertical</item>
		<item name="android:background">@color/transparent</item>
		<item name="android:contentDescription">@null</item>
		<item name="android:textSize">15sp</item>
		<item name="android:fontFamily">@font/os_semi_bold</item>
		<item name="android:textColor">@color/white</item>
		<item name="android:textAllCaps">true</item>
	</style>

	<style name="OrangeButton">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">44dp</item>
		<item name="android:layout_marginBottom">25dp</item>
		<item name="android:background">@drawable/gradient_orange_vertical</item>
		<item name="android:fontFamily">@font/os_semi_bold</item>
		<item name="android:textSize">18sp</item>
		<item name="android:textColor">@color/white</item>
		<item name="android:textAllCaps">false</item>
		<item name="android:elevation">6dp</item>
	</style>

	<style name="NewOrangeButton">
		<item name="android:layout_width">0dp</item>
		<item name="android:layout_height">43.7dp</item>
		<item name="android:layout_marginBottom">20dp</item>
		<item name="android:background">@drawable/gradient_orange_vertical</item>
		<item name="android:fontFamily">@font/os_semi_bold</item>
		<item name="android:textSize">16.7sp</item>
		<item name="android:textColor">@color/white</item>
		<item name="android:textAllCaps">false</item>
		<item name="android:elevation">6dp</item>
	</style>

	<style name="LargeOrangeButton">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">@dimen/custom_44dp</item>
		<item name="android:background">@drawable/large_orange_background</item>
		<item name="android:fontFamily">@font/os_bold</item>
		<item name="android:textSize">@dimen/large_orange_size</item>
		<item name="android:textColor">@color/white</item>
		<item name="android:textAllCaps">false</item>
	</style>

	<style name="OrangeButton_15">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">43.7dp</item>
		<item name="android:layout_marginBottom">20dp</item>
		<item name="android:background">@drawable/gradient_orange_vertical</item>
		<item name="android:fontFamily">@font/os_semi_bold</item>
		<item name="android:textSize">15sp</item>
		<item name="android:textColor">@color/white</item>
		<item name="android:textAllCaps">false</item>
		<item name="android:elevation">6dp</item>
	</style>

	<style name="BottomSheetButtonStyle">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:fontFamily">@font/os_semi_bold</item>
		<item name="android:textAllCaps">true</item>
		<item name="android:textColor">@color/dark_gray_transparent</item>
		<item name="android:textSize">15sp</item>
	</style>

	<style name="BottomSheetTitleStyle">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:fontFamily">@font/os_semi_bold</item>
		<item name="android:textColor">@color/dark_black_transparent</item>
		<item name="android:textSize">18sp</item>
	</style>

	<style name="CircuitPickerContainerStyle">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:padding">5dp</item>
		<item name="android:background">@drawable/gray_stroke_rounded_button_background</item>
	</style>

	<style name="CircuitSmallPickerContainerStyle" parent="CircuitPickerContainerStyle">
		<item name="android:minWidth">45dp</item>
	</style>

	<style name="CircuitMediumPickerContainerStyle" parent="CircuitPickerContainerStyle">
		<item name="android:minWidth">62dp</item>
	</style>

	<style name="CircuitLargePickerContainerStyle" parent="CircuitPickerContainerStyle">
		<item name="android:minWidth">74dp</item>
	</style>

	<style name="ProgramViewTextStyle">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:fontFamily">@font/os_semi_bold</item>
		<item name="android:textSize">17sp</item>
		<item name="android:textColor">@color/black</item>
		<item name="android:textAllCaps">true</item>
	</style>

	<style name="ProgramViewDetailTitleStyle" parent="ProgramViewTextStyle">
		<item name="android:textColor">@color/colorPrimary</item>
		<item name="android:textSize">15sp</item>
		<item name="android:textAllCaps">false</item>
	</style>

	<style name="ProgramViewDetailStyle" parent="ProgramViewTextStyle">
		<item name="android:layout_width">0dp</item>
		<item name="android:fontFamily">@font/os</item>
		<item name="android:textColor">@color/black</item>
		<item name="android:textSize">15sp</item>
		<item name="android:textAllCaps">false</item>
	</style>

	<style name="ProgramInfoInputContainerStyle">
		<item name="android:layout_width">0dp</item>
		<item name="android:layout_height">48dp</item>
		<item name="android:paddingStart">15sp</item>
		<item name="android:paddingEnd">15dp</item>
		<item name="android:fontFamily">@font/os</item>
		<item name="android:gravity">center_vertical</item>
		<item name="android:textSize">15sp</item>
		<item name="hintTextColor">@color/dark_gray</item>
		<item name="android:textColor">@color/black</item>
		<item name="android:background">@drawable/gray_stroke_rounded_background</item>
	</style>
	
	<style name="ProgramDetailValueTextStyle">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:fontFamily">@font/os</item>
		<item name="android:textSize">13sp</item>
		<item name="android:textColor">@color/black</item>
	</style>

	<style name="LoginInputContainerStyle">
		<item name="android:layout_width">0dp</item>
		<item name="android:layout_height">@dimen/custom_50dp</item>
		<item name="android:layout_marginStart">@dimen/custom_40dp</item>
		<item name="android:layout_marginEnd">@dimen/custom_40dp</item>
		<item name="android:layout_marginBottom">@dimen/custom_8dp</item>
		<item name="cardCornerRadius">@dimen/custom_25dp</item>
		<item name="cardElevation">4dp</item>
	</style>

	<style name="DialogStyle">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
	</style>

	<style name="DialogTitleStyle">
		<item name="android:layout_width">0dp</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:fontFamily">@font/os_semi_bold</item>
		<item name="android:textSize">19sp</item>
		<item name="android:textColor">@color/black</item>
		<item name="android:textAlignment">center</item>
	</style>

	<style name="DialogButtonStyle">
		<item name="android:layout_width">180dp</item>
		<item name="android:layout_height">39dp</item>
		<item name="android:fontFamily">@font/os_semi_bold</item>
		<item name="android:textAllCaps">true</item>
		<item name="android:textSize">15sp</item>
		<item name="android:textColor">@color/white</item>
		<item name="android:background">@drawable/gradient_blue_vertical</item>
	</style>

	<style name="BottomSheetNumberPickerStyle">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="np_dividerColor">@color/transparent</item>
		<item name="np_textSize">17sp</item>
		<item name="np_selectedTextSize">17sp</item>
		<item name="np_textColor">@color/picker_value_color</item>
		<item name="np_selectedTextColor">@color/black</item>
		<item name="np_wheelItemCount">5</item>
	</style>

	<style name="CircuitRoundItemViewCardHolderStyle">
		<item name="android:layout_width">0dp</item>
		<item name="android:layout_height">112dp</item>
		<item name="android:layout_marginStart">10dp</item>
		<item name="android:layout_marginEnd">14dp</item>
		<item name="android:layout_marginBottom">7dp</item>
		<item name="cardCornerRadius">@dimen/default_corner_radius</item>
		<item name="cardBackgroundColor">@color/transparent</item>
		<item name="cardElevation">0dp</item>
	</style>

	<style name="HelpTextStyle">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:fontFamily">@font/os</item>
		<item name="android:textSize">@dimen/custom_14sp</item>
	</style>

	<style name="ProgramViewCardioHeaderTextStyle" parent="Widget.AppCompat.TextView">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:textAlignment">center</item>
		<item name="android:textColor">@color/white</item>
		<item name="android:fontFamily">@font/os_semi_bold</item>
		<item name="android:maxLines">1</item>
		<item name="android:textSize">@dimen/custom_18sp</item>
	</style>

	<style name="ProgramReceivedItemTextStyle" parent="Widget.AppCompat.TextView">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:textColor">@color/black</item>
		<item name="android:fontFamily">@font/os</item>
		<item name="android:textSize">@dimen/custom_13sp</item>
	</style>

	<color name="text_color_black">#010101</color>
</resources>
